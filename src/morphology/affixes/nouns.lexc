! ======================
!! !!!Sublexica for NounRoot
! ======================

!! Aloitin sovittaa analyysin Eiran kieliopin mukaan (Porsanki).
!! Leksikot ovat nyt (tai pitää olla) n11, n12, jne., eli siis
!! Eiran nominit tyyppi 1.1., 1.2., jne.

!!  2007 s.87
!! * 1. Yksitavuiset / Vi ( maa, tuorestai)                 = n_1
!! * 1.1. Yksitavuiset monikko/ Vi ( häät)                 = n_1pl
!! * 2. Vokaalirankaiset
!! ** 2.1 Kaksitavuiset ( pappi)                             = n_21
!! *** 2.1.1. Kaksitavuiset monikko (housut)     = n_21pl
!! *** 2.1.2. Kaksitavuiset (joki: joke)         = n_21ie
!! ** 2.2. Usseempitav. (opettaaja)                               = n_22
!! *** 2.2.1. Usseempitav. odd (kajava)                  = n_22odd
!! *** 2.2.2. Usseempitav. odd (meininki, tunturi)        = n_22oddi
!! *** 2.2.3. Usseempitav. monikko oi (kulkkiija)        = n_22oi
!! ** 2.3 Kokkoontumanom. ( valkkee)                     = n_23
!! ** 2.3.1 Kokkoontumanom. (assii)             = n_23ii
!! * 3. Konsonanttirankaiset
!! ** 3.1 Kaksitavuiset ( pieni, kieli)                         = n_31
!! ** 3.1.1. Kaksitavuiset lumi)                 = n_31m
!! ** 3.1.2. (käsi, vuosi)                                                 = n_31si
!! ** 3.1.3. (lapsi)                                                      = n_31lapsi
!! ** 3.1.4. (mies)                                                         = n_31mies
!! ** 3.2 Usseempitavuiset
!! ** 3.2.1 (tytär)                              = n_32
!! ** 3.2.2. (elläin)                            = n_32m
!! ** 3.2.3 (rakkhaus)                                                         = n_32s
!! ** 3.3 Kokkoontumanom. ( opas, huonet)
!! ** 3.3.1 (opas)                               = n_33s
!! ** 3.3.2 (asukas)                             = n_33sodd
!! ** 3.3.3. (ruvis)                           = n_33is
!! ** 3.3.4 (huonet)                            = n_33t
!! ** 3.3.5 (kirves)                                                         = n_33ts
!! * 4. Ekstrakonsonanttirankaiset
!! ** 4.1 veres-nominit ( veres, kokkous)                 = n_41
!! ** 4.1.1. veres-nominit monikko ( veljekset)                = n_41pl
!! ** 4.2 –inen-nominit ( kainulainen)                         = n_42 = nen
!! *** 4.2.1 -inen-nominit Pl (tervheiset)                 = n_42pl = nen
!! *** 4.2.2 -inen-nominit odd (vihrinen)                 = n_42odd = nen

!! !!Noun types

!! !!!INCOMING

LEXICON n_x
 +N: x_x ;

LEXICON x_x
 +Sg+Nom: K ; ! Just to make it compile



LEXICON n_1 !!= * @CODE@ = maa, tie
             +N: x_1 ;
      +N+Sg+Nom: R   ;
 +N+Dial/Por+Pl:%>i2ne   gen_n           ;

 LEXICON n_1ai !!= * @CODE@ = tuorestai ja muut päivät
         +N: x_1 ;
  +N+Sg+Nom: R   ;

LEXICON x_1
  +Sg+Nom:         K               ;
      +Sg:         gen_n_sg        ;
      +Sg:         ill_hVn         ; ! maahan
      +Sg:         par_tA          ;
      +Sg:         sgpl_cases_even ; !! these two lexica for cases with
         :         x_1pl           ;
! +Sg+Nom:         Px              ; ! mattoni
! +Sg+Gen:         Px              ; ! mattoni
! +Pl+Nom:         Px              ; ! mattoni
      +Sg:         ess_n           ;



LEXICON n_1pl !!= * @CODE@ = häät
        +N: x_1 ;

LEXICON x_1pl !!= * @CODE@ = häät
       +Sg: gen_n_sg ;
       +Pl: nom_t    ;
       +Pl:%>i2      sgpl_cases_even ; !! the same affix in sg and pl

         +Pl:%>i2     gen_tten       ;
+Dial/Por+Pl:%>i2%>ne gen_n          ; ! häinen

       +Pl:%>i2      par_tA          ;
       +Pl:%>i2      ill_hin         ;
       +Pl:%>i2      ess_n           ;
       +Pl:%>i2      com_ne          ;

LEXICON n_21C !!= * @CODE@
 +N+Sg+Nom:  K    ; ! Hans
          :i n_21 ; ! Hansi
 +N+Sg+Nom:i R    ;

LEXICON n_21        !!= * @CODE@  "poika:poi6ka", lyhyt vokali, yks vartalo, kuva, linna, pappi
        +N: x_21 ;
 +N+Sg+Nom: R    ;

LEXICON x_21 ! "poika:poi6ka", lyhyt vokali, yks vartalo, kuva, linna, pappi, tooli, syksy
+Sg+Nom:    K    ;
            x_21_oblique ;

LEXICON x_21_oblique
 +Dial/Por+Sg:                 par_V           ;
+Dial/-Por+Sg:                 par_A           ;
          +Sg:                 ess_n           ;
          +Pl:^AO%>i2          ess_n           ;
          +Sg:^WG              gen_n_sg        ;
          +Pl:^WG              nom_t           ;
          +Sg:^WG              sgpl_cases_even ; !to sgpl_cases_even for cases with
          +Pl:^AO^WG%>i2       sgpl_cases_even ; !the same affix in sg and pl
          +Sg:^HMETA           ill_hVVn        ;
 +Dial/Por+Pl:^AO%>i2          gen_n           ; ! gen_n         since we have the i
+Dial/-Por+Pl:^AO^WG%>i2       gen_tten        ;! syksyitten, toolitten. Dialect ! longgenpl, eira 5322, korpus.
 +Dial/Por+Pl:^AO%>i2          par_end         ;
+Dial/-Por+Pl:^AO%>j           par_A           ; ! remove one
+Dial/-Por+Pl:^AO%>i2          par_A           ; ! remove one

        +Pl:^AO^HMETA          ill_hVin        ; ! sykshyin
        +Pl:^AO%>i2            com_ne          ;


LEXICON n_21pl        !!= * @CODE@  "villahousut", lyhyt vokali, yks vartalo, kasvot
           +N:       x_21pl    ;
        +N+Pl:       nom_t     ; ! housut, kasvot

LEXICON x_21pl        !!= * @CODE@

          +Pl:%>i2               sgpl_cases_even ; !! the same affix in sg and pl

 +Dial/Por+Pl:%>i2               gen_n           ; ! kasvoin
+Dial/-Por+Pl:%>i2               gen_tten        ; ! kasvoitten, housuitten
 +Dial/Por+Pl:%>i2               par_end         ; ! kasvoi
+Dial/-Por+Pl:%>j                par_A           ; ! kasvoja

          +Pl:^AO^HMETA          ill_hVin        ;
          +Pl:%>i2               ess_n           ;
          +Pl:%>i2               com_ne          ;


! LEXICON kk-longgenpl
!      +Gen:%>kk%>i2           #               ; ! kk ?? Tsekkaa..

!LEXICON kk-longgparpl
! +Par:%>kk%>i2 # ; ! kk ?? Tsekkaa..

LEXICON n_21ie        !!= @CODE@ on ove, ovi, nom joki:joke, hyksi, suomi
        +N:     x_21ie ;
 +N+Sg+Nom:^E2I R      ; ! ^EI e to i

LEXICON x_21ie
      +Sg+Nom:^E2I     K               ; ! ^EI e to i
 +Dial/Por+Sg:         par_V           ; ! suomee
+Dial/-Por+Sg:^E2I     par_A           ; ! suomea
          +Sg:^WG      gen_n_sg        ;
          +Pl:^WG      nom_t           ;
          +Sg:^WG      sgpl_cases_even ; !! these two lexica for cases with
          +Sg:         ess_n           ;
          +Pl:^WG%>i2  sgpl_cases_even ; !! the same affix in sg and pl
 +Dial/Por+Pl:%>i2     gen_Vn          ; ! hyksiin
+Dial/-Por+Pl:%>i2     gen_tten        ; ! hyksitten
 +Dial/Por+Pl:%>i2     par_V           ; ! hyksii
+Dial/-Por+Pl:%>i2     par_A           ; ! hyksiä
          +Sg:^HMETA   ill_hVVn        ;
!      +Pl+Ill:^EDEL%>hii n_PxK         ; ! TODO add Twolc description about EDEL ! suomhiin
          +Pl:^EDEL    ill_hiin        ; ! TODO add Twolc description about EDEL ! suomhiin

          +Pl:%>i2     ess_n           ;
          +Pl:%>i2     com_ne          ;




LEXICON n_22        !!= * @CODE@  opettaaja, karamelli
        +N: x_22 ;
 +N+Sg+Nom: R    ;

LEXICON x_22
   +Sg+Nom:            K                  ;
 +Dial/Por+Sg:         par_V              ;
+Dial/-Por+Sg:         par_A              ; ! karamellia
       +Sg:            ess_n              ; ! karamellina
       +Sg:^WG         gen_n_sg           ;
       +Pl:^WG         nom_t              ;
       +Sg:^WG         sgpl_cases_even    ; ! the same affix in sg and pl
 +Dial/Por+Pl:^WG%>i2  sgpl_cases_even    ; ! Adding  to block i:0
+Dial/-Por+Pl:^WG%>    sgpl_cases_even    ; ! Adding  to block i:0
          +Sg:^HMETA   ill_hVVn           ;
          +Pl:^HMETA   ill_hiin           ;
!   +Pl:^WG%>i2       gen_tten           ;
!   +Pl:%>i2          par_tA             ;
!   +Pl:%>i2          par_end            ; !liberaali, dialissa on liberaalia?
!   +Pl:%>i2          ess_n                ;
!   +Pl:%>i2          com_ne             ;
        +Pl:           x_22_pl_por        ;
        +Pl:           x_22_pl_var        ;

 LEXICON x_22_pl_por
  +Dial/Por:^WG%>i2     gen_tten           ;
  +Dial/Por:%>i2        par_tA             ;
  +Dial/Por:%>i2        par_end            ;
  +Dial/Por:%>i2        ess_n                ;
  +Dial/Por:%>i2        com_ne             ;

 LEXICON x_22_pl_var
 +Dial/-Por:^WG%>       gen_tten           ;
 +Dial/-Por:%>          par_A              ;
 +Dial/-Por:%>          ess_n                ; ! karamelline
 +Dial/-Por:%>          com_ne             ; ! karamellina

! +Pl:%>i2        gen_n              ; opettaajiin
! +Pl+Tra:%>i2ksi K ; Lene removed this one
! !+Pl+Tra:%>i2kse     PxxK               ;

LEXICON n_22odd !!= * @CODE@  kajava
      +N:  x_22odd ;
 +Sg+Nom:  K       ;

LEXICON x_22odd !!= * @CODE@  kajava
          +Sg:^WG          sgpl_cases_odd ;
          +Sg:^WG          gen_n_sg       ;
 +Dial/Por+Sg:             par_V          ;
+Dial/-Por+Sg:             par_A          ;
          +Sg:^HMETA       ill_hVVn       ;
          +Sg:             ess_nn         ;

          +Pl:^WG%>i2      sgpl_cases_odd ; ! Adding  to block i:0
          +Pl:^WG          nom_t          ;
 +Dial/Por+Pl:^WG%>i2i     gen_tten       ; ! kajaviitten
+Dial/-Por+Pl:^WG%>i2      gen_tten       ; ! kajavitten
 +Dial/Por+Pl:%>i2i        par_tA         ; ! kajaviita
 +Dial/Por+Pl:%>i2%>i      par_end        ; ! %> i necessary, otherwise a:o NOTwork, kajavii
+Dial/-Por+Pl:%>i2%>       par_A          ; ! kajavia
!      +Pl+Ill:%^HMETA%>hii n_PxK          ;
          +Pl:%^HMETA      ill_hiin       ;
          +Pl:%>i2         ess_nn         ;
          +Pl:%>i2         com_ne         ; ! kajavine
! +Pl+Tra:%>i2%>ksi    PxxK           ;



LEXICON n_22oddi !!= * @CODE@  meininki, tunturi
        +N:     x_22oddi ;
 +N+Sg+Nom:     R        ;
     +N+Sg:^WG  gen_n    ;

LEXICON x_22oddi !!= * @CODE@  meininki, tunturi
          +Sg+Nom:        K               ;
              +Sg:^WG     sgpl_cases_odd  ;
              +Sg:^WG     gen_n_sg        ;
     +Dial/Por+Sg:        par_V           ;
    +Dial/-Por+Sg:        par_A           ;
              +Sg:^HMETA  ill_hVVn        ;
              +Sg:        ess_nn          ;

              +Pl:^WG     nom_t           ;
     +Dial/Por+Pl:^WG%>i2 sgpl_cases_even ; ! Adding  to block i:0
    +Dial/-Por+Pl:^WG%>   sgpl_cases_odd  ; ! Adding  to block i:0
     +Dial/Por+Pl:^WG%>i2 gen_tten        ;
    +Dial/-Por+Pl:^WG%>   gen_tten        ;
     +Dial/Por+Pl:%>i2    par_tA          ;
    +Dial/-Por+Pl:%>      par_A           ;
              +Pl:^HMETA  ill_hiin        ;
              +Pl:        ess_nn          ;
     +Dial/Por+Pl:i2      com_ne          ;
    +Dial/-Por+Pl:        com_ne          ;


!  Tarkista, onko yaml oikein:
