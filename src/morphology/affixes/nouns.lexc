                           nom_strong     ;
                           short_par      ;
             :^WG          weak_v_stem    ;
          +Sg:^WG          sgpl_cases_odd ;
          +Pl:^WG%>i2      sgpl_cases_odd ; ! Adding  to block i:0
                           2TAVU_V_ill_sg ;
          +Sg:             ess_nn         ;
 +Pl+Dial/Por:^WG%>i2i     tt_longgenpl   ; ! kajaviitten
+Pl+Dial/-Por:^WG%>i2      tt_longgenpl   ; ! kajavitten
 +Pl+Dial/Por:%>i2i        longparpl      ; ! kajaviita
 +Pl+Dial/Por:%>i2%>i      shortestparpl  ; ! %> i necessary, otherwise a:o NOTwork, kajavii
+Pl+Dial/-Por:%>i2%>       shortparpla    ; ! kajavia
      +Pl+Ill:%^HMETA%>hii n_PxK          ;
          +Pl:%>i2         ess_nn         ;
          +Pl:%>i2         com            ; ! kajavine
! +Pl+Tra:%>i2%>ksi    PxxK           ;






