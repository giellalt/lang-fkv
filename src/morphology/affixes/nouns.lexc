!                     nomgen_px       ;
         :^WG         weak_v_stem     ;
      +Sg:^WG         sgpl_cases_odd  ;

 +Pl+Dial/Por:^WG%>i2  sgpl_cases_even ; ! Adding  to block i:0
+Pl+Dial/-Por:^WG%>   sgpl_cases_odd  ; ! Adding  to block i:0
                      2TAVU_V_ill_sg  ;
          +Sg:        ess_nn          ;
          +Pl+Ill:^HMETA%>hii n_PxK   ;
          +Pl:        ess_nn          ;

 +Pl+Dial/Por:^WG%>i2  tt_longgenpl   ;
