LEXICON n_31        !!= @CODE@  kieli
        +N:     x_31 ;
 +N+Sg+Nom:^E2I R    ; ! ^E2I e to i

LEXICON x_31
  +Sg+Nom:^E2I        K               ; ! ^E2I e to i
      +Sg:^EDEL       par_tA          ;
      +Sg:^WG         gen_n_sg        ;
      +Pl:^WG         nom_t           ;
      +Sg:^WG         sgpl_cases_even ; !! * @CODE@  for same affix in sg and pl
      +Sg:            ess_n           ;
      +Pl:%>i2        sgpl_cases_even ; !! * @CODE@  for same affix in sg and pl
      +Sg:^HMETA      ill_hVVn        ;
      +Pl:^HMETA      ill_hiin        ;

 +Dial/Por+Pl:%>i2i   gen_n           ;
+Dial/-Por+Pl:        gen_ten         ; ! kielten
+Dial/-Por+Pl:%>i2    gen_tten        ; ! kielitten, sienitten
