+N+Pl:%^WG%>i2 sgpl-cases-short ;  !!   the same affix in sg and pl
+N: 2TAVU_V_ill_sg ;
+N+Pl:%>i2 shortestgenpl ;  !! * to shortestgenpl since we already
+N+Pl:%>i2 shortestparpl  ; !!   have the i
+N+Pl:%>%^HMETAh%^Vi2 shortestillpl ;


LEXICON n_21pl	!!= * @CODE@  "poika", lyhyt vokali, yks vartalo
