LEXICON n_22oi        !!= * @CODE@  Kulkkiija, paivukko, sivakka, perintö, kurkkio
 +N: x_22oi ;

LEXICON x_22oi
      +Sg+Nom:                   K               ; ! sg nom
 +Dial/Por+Sg:                   par_V           ;
+Dial/-Por+Sg:                   par_A           ;
          +Sg:^WG                gen_n_sg        ;
          +Pl:^WG                nom_t           ;
          +Sg:^WG                sgpl_cases_odd  ; ! these two lexica for cases with
          +Pl:^AO^WG%>i2         sgpl_cases_even ; ! the same affix in sg and pl
          +Sg:^HMETA             ill_hVVn        ;
          +Pl:^AO^WG%>i2         gen_tten        ;
          +Pl:^AO^WG%>i2         par_tA          ;
          +Sg:                   ess_nn          ; ! paivukkonna
          +Pl:^AO^HMETA          ill_hVin        ;
          +Pl:^AO%>i2            ess_n           ;! paivukoina, TODO kulkijoina ei toimi jos on ^AO, miksi?
          +Pl:^AO%>i2            com_ne          ;! paivukoine, TODO kulkijoine ei toimi jos on ^AO, miksi?
