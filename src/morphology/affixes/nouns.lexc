      +Sg+Nom:        K               ;
          +Sg:        par_tA          ;
          +Sg:        ill_sseen       ;
          +Pl:        ill_ssiin       ;
          +Sg:        ess_n           ;
 +Dial/Por+Pl:        gen_tten        ;
+Dial/-Por+Pl:^AO%>i2 gen_tten        ;
