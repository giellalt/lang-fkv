    +Dial/Por:         short_par       ; ! suomee
   +Dial/-Por:^E2I     short_par_a     ; ! suomea
                       obl_strong_even ;
             :^WG      weak_v_stem     ;
          +Sg:^WG      sgpl_cases_even ; !! these two lexica for cases with
          +Pl:^WG%>i2  sgpl_cases_even ; !! the same affix in sg and pl
 +Pl+Dial/Por:%>i2     shortgenpl      ; ! hyksiin
+Pl+Dial/-Por:%>i2     tt_longgenpl    ; ! hyksitten
 +Pl+Dial/Por:%>i2     shortparpl      ; ! hyksii
+Pl+Dial/-Por:%>i2     shortparpla     ; ! hyksiä
                       2TAVU_edel-ill  ;
          +Pl:%>i2     com             ;


LEXICON n_22        !!= * @CODE@  opettaaja, karamelli
        +N: x_22 ;
 +N+Sg+Nom: R    ;

LEXICON x_22
                       nom_strong         ;
                       short_par          ; ! karamellia
       +Sg:            ess_n              ; ! karamellina
          :^WG         weak_v_stem        ;
       +Sg:^WG         sgpl_cases_even    ; ! the same affix in sg and pl
 +Pl+Dial/Por:^WG%>i2  sgpl_cases_even    ; ! Adding  to block i:0
+Pl+Dial/-Por:^WG%>    sgpl_cases_even    ; ! Adding  to block i:0
                       2TAVU_V_ill_sg     ;
   +Pl+Ill:^HMETA%>hii n_PxK              ;
!   +Pl:^WG%>i2       tt_longgenpl       ;
!   +Pl:%>i2          longparpl          ;
!   +Pl:%>i2          shortestparpl      ; !liberaali, dialissa on liberaalia?
!   +Pl:%>i2          ess_n                ;
!   +Pl:%>i2          com                ;
        +Pl:           x_22_pl_por        ;
        +Pl:           x_22_pl_var        ;

 LEXICON x_22_pl_por
  +Dial/Por:^WG%>i2     tt_longgenpl       ;
  +Dial/Por:%>i2        longparpl          ;
  +Dial/Por:%>i2        shortestparpl      ;
  +Dial/Por:%>i2        ess_n                ;
  +Dial/Por:%>i2        com                ;

 LEXICON x_22_pl_var
 +Dial/-Por:^WG%>       tt_longgenpl       ;
 +Dial/-Por:%>          shortparpla        ;
 +Dial/-Por:%>          ess_n                ; ! karamelline
 +Dial/-Por:%>          com                ; ! karamellina

! +Pl:%>i2        shortestgenpl      ; opettaajiin
! +Pl+Tra:%>i2ksi K ; Lene removed this one
! !+Pl+Tra:%>i2kse     PxxK               ;

LEXICON n_22odd !!= * @CODE@  kajava
