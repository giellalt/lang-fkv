!+A+Comp:%>m^A weak_v_stem ;
!+A+Pl+Comp+Dial/Por:%>m%>i2i  gen_tten ; ! liberaalimitten ! used2be +Gen:%>m%>i2%>itten K ;
!+A+Pl+Comp+Dial/Jok:%>m%>i2  gen_tten ; ! liberaalimiitten i2 or i ?
!+A+Pl+Comp+Dial/Por:%>mpp%>i2%>i par_tA   ; ! used2be +Par:%>mpp%>i2%>it^A K ;
