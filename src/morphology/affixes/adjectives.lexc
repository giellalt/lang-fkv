LEXICON comp_m_odd  ! odd here meaning the resulting stem: 2syll + comp = 3syll
+Sg+Comp:%>mp^A    ess_nnA         ;
+Sg+Comp:%>m^A     sgpl_cases_odd  ;  ! ade, all, ine, ela, abl, tra

+Pl+Comp:%>mp%>i2  ess_nnA         ;
+Pl+Comp:%>mp%>i2  com_nne         ;
+Pl+Comp:%>m%>i2   sgpl_cases_odd  ;
                   comp_m_common   ;

LEXICON comp_m_even  ! split for syllabicity sinnikkhäämälä
+Sg+Comp:%>mp^A    ess_nA          ;
+Sg+Comp:%>m^A     sgpl_cases_even ;  ! ade, all, ine, ela, abl, tra

+Pl+Comp:%>mp%>i2  ess_nA          ;
+Pl+Comp:%>mp%>i2  com_ne          ;
+Pl+Comp:%>m%>i2   sgpl_cases_even ;
                   comp_m_common   ;

