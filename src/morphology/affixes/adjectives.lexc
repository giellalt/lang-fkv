LEXICON comp_m_odd  ! odd here meaning the resulting stem: 2syll + comp = 3syll
  +Sg+Compar      :%>mp^A    ess_nnA         ;
  +Sg+Compar      :%>m^A     sgpl_cases_odd  ;  ! ade, all, ine, ela, abl, tra

  +Pl+Compar      :%>mp%>i2  ess_nnA         ;
  +Pl+Compar      :%>mp%>i2  com_nne         ;
  +Pl+Compar      :%>m%>i2   sgpl_cases_odd  ;
                   comp_m_common   ;

LEXICON comp_m_even  ! split for syllabicity sinnikkhäämälä
  +Sg+Compar      :%>mp^A    ess_nA          ;
  +Sg+Compar      :%>m^A     sgpl_cases_even ;  ! ade, all, ine, ela, abl, tra

  +Pl+Compar      :%>mp%>i2  ess_nA          ;
  +Pl+Compar      :%>mp%>i2  com_ne          ;
  +Pl+Compar      :%>m%>i2   sgpl_cases_even ;
                   comp_m_common   ;

LEXICON comp_m_common
