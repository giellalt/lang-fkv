<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE errors>

<errors>

  <defaults>
    <default type="Grammar error">
      <ids>
        <re v="real-.*"/>
        <e id="unreal-girjji"/>
      </ids>
      <header>
        <title xml:lang="en">Real-word error</title>
        <title xml:lang="fkv">Čállinmeattáhus</title>
      </header>
      <body>
        <description xml:lang="en">The word "$1" exists in the dictionary, but means something different in this context.</description>
        <description xml:lang="fkv">"$1" maŋis galggašii leat blah</description>
      </body>
    </default>

    <default type="Grammar error">
      <ids>
        <e id="default"/>
      </ids>
      <header>
        <title xml:lang="en">Grammar error</title>
        <title xml:lang="fkv">Kielioppivirhe</title>
      </header>
      <body>
        <description xml:lang="en">The word "$1" exists in the dictionary, but means something different in this context.</description>
        <description xml:lang="fkv">Sana "$1" on olemassa, mutta tässä kontekstissa se ei sovi.</description>
      </body>
    </default>

    <default type="Style error">
      <ids>
        <re v="space-.*"/>
        <re v=".*-space-.*"/>
      </ids>
      <header>
        <title xml:lang="en">Extra spaces</title>
        <title xml:lang="fkv">Lisää välejä</title>
      </header>
      <body>
        <description xml:lang="en">There should be no space between "$1" and the punctuation mark.</description>
        <description xml:lang="fkv">"$1" ja välimerkin välilla ei saa tyhjä merkki.</description>
      </body>
    </default>
  </defaults>

  

  <error id="msyn-agr-sg2-sg1">
    <header>
      <title xml:lang="en">First person singular after 'mun'.</title>
      <title xml:lang="fkv">Verbi ensimmäisessä persoonassa.</title>
    </header>
    <body>
      <description xml:lang="en">The verb-form "$1" follows 'mun', and should probably be first person singular.</description>
      <description xml:lang="fkv">Verbimuotoa "$1" käytetään 'mie'-sanan jälkeen. Olisiko ensimmäinen persoona parempi?</description>
    </body>
  </error>

  <error id="msyn-agr-sg3-pl3">
    <header>
      <title xml:lang="en">Singular verb should be plural</title>
      <title xml:lang="fkv">Yksikön verbi pitäisi olla monikossa</title>
    </header>
    <body>
      <description xml:lang="en">The verb-form "$1" should probably be in the plural.</description>
      <description xml:lang="fkv">Verbimuotoa "$1" -- olisiko monikko parempi?</description>
    </body>
  </error>

  <error id="msyn-agr-pl3-sg3">
    <header>
      <title xml:lang="en">Plural verb should be singular</title>
      <title xml:lang="fkv">Monikon verbi pitäisi olla yksikössä</title>
    </header>
    <body>
      <description xml:lang="en">The verb-form "$1" should probably be in the singular.</description>
      <description xml:lang="fkv">Verbimuotoa "$1" -- olisiko yksikkö parempi?</description>
    </body>
  </error>

  <error id="msyn-agr-prfprcsg-prfprcpl">
    <header>
      <title xml:lang="en">'Plural participle after verb in plural</title>
      <title xml:lang="fkv">Monikon partisiippi monikkoverbin jälkeen</title>
    </header>
    <body>
      <description xml:lang="en">The participle "$1" follows a plural verb "$2", and should probably be in the plural.</description>
      <description xml:lang="fkv">Partisiippi "$1" tullee monikkoverbin "$2" jälkeen. Olisiko monikko parempi?</description>
    </body>
  </error>


  <error id="msyn-neg-posv">
    <header>
      <title xml:lang="en">Negative verbform after negative verb.</title>
      <title xml:lang="fkv">Kieltomuoto kieltoverbin jälkeen.</title>
    </header>
    <body>
      <description xml:lang="en">The verb "$1" follows a negative verb, and should thus have the negative form.</description>
      <description xml:lang="fkv">Verbi "$1" tulee kieltoverbin jälkeen, ja pitäisi sen takia olla kieltomuodossa.</description>
    </body>
  </error>

  <error id="msyn-agr-sg1">
